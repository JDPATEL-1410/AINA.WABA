{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server.js"
        },
        {
            "source": "/webhook",
            "destination": "/server.js"
        }
    ],
    "functions": {
        "server.js": {
            "includeFiles": "dist/**"
        }
    }
}